		event = { time = 0 node="muzzle" particle = "mg_muzzle_smoke_particle"  keep_particle = yes   light = "mg_muzzle_flash" sound = { soundeffect = m16_attack } } 
		event = { time = 0 node="cartridge" particle = "mg_cartridge_particle"  keep_particle = yes } 
